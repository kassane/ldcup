{
	"name": "ldcup",
	"version": "1.1.6",
	"description": "Download and manage D compiler [ldc2].",
	"license": "Apache-2.0",
	"targetPath": "bin",
	"authors": [
		"Matheus Catarino Fran√ßa"
	],
	"dependencies": {
		"requests": {
			"repository": "git+https://github.com/bagomot/dlang-requests.git",
			"version": "ba31d83a1cbab585b530d475bfaca37bc6a4efed"
		}
	},
	"dflags": [
		"-preview=dip1000",
		"-preview=dip1008",
		"-preview=rvaluerefparam",
		"-preview=in",
		"-preview=systemVariables"
	],
	"buildTypes": {
		"debug": {
			"buildOptions": [
				"debugMode",
				"debugInfo",
				"debugInfoC"
			]
		},
		"release": {
			"buildOptions": [
				"optimize",
				"inline"
			]
		},
		"release-debug": {
			"buildOptions": [
				"optimize",
				"inline",
				"debugInfo",
				"debugInfoC"
			]
		},
		"release-nobounds": {
			"buildOptions": [
				"optimize",
				"inline",
				"noBoundsCheck"
			]
		}
	}
}